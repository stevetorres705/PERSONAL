// VARABLIES GO HERE !!!
var heal = 10 
  


//
// FUNCS GO HERE 


  
  
  
//

// func to tell what weakness the foe has
func DisplayWeak(x)  
  ?x = aether & 
  ^ x = vigor & 
  ^ x = poison & 
  ^ x = ice &
  ^ x = fire 
    return ITS DYSAN JUMP HIM
  ?x = aether
    return vigor
  :?x = vigor
    return poison
  :?x = poison
    return ice
  :?x = ice 
    return fire
  :?x = fire 
    return aether
  :
    return none
//
func HEAL(h)
  ?hp = maxhp | heal > 500
    heal = 10

  :?hp <= maxhp / 2 | h > 10
    heal++
    Equipl vigor sw
    Equipr vigor wand l
  
func DYSANPROT()
  
  
  ?foe = phase1 
    Equip bard
   
  :?foe = phase2
    ?foe = vigor 
      Equipl Poison
      Equipr poison w
    :?foe = poison 
      equipl Ice 
      Equipr ice wa
    :?foe = ice 
      Equipl Fire 
      Equipr fire wa
    :?foe = fire
      Equipl aether 
      Equipr aether w
    :?foe = aether 
      Equipl vigor 
      Equipr vigor w
   
  ?foe = phase3 
    ?foe.state >= 115 
      // IM FIRING MAH LASER
      ?foe.state = 115 & foe.time = 49
        Equip mind 
        //WEAVE
      :?foe.state = 115 
        ?foe.time > 50 &
        ^ foe.time <= 65
          equip bash
          //LAYOUT THAT MF
        :
          Equip bard
       
    :
      Equip bard
   
  
 
  HEAL(heal)
      
// 
