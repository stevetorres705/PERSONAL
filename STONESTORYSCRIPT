var BARD = ＂Bardiche +11 8*＂
?loc = deadwood & loc.stars > 3// works on 7*
  ?foe = boss 
    
    equip bar
    
    
    ?foe.state >= 32 & foe ! immune_to_stun
      Equipr co
      Equipl stone Hammer
     
    
    
  :?foe.distance >= 10 & foe ! immune_to_ranged
     Equip poison bow
    
  :?foe.count >= 3 | foe = immune_to_ranged
     Equipl vigor
     Equipr co
    

 

    

 
//
?loc = caves // works on 7 *

  ?foe = boss
    
    Equipr ice
    Equipl vigor
  :?foe.distance < 10 | foe.count >= 4
    Equip bar
  :?foe.distance > 10 & foe.count <= 1
    Equip re
  HEAL(heal) 

//
?loc = icy // works on 5*
  ?foe = boss
    ?foe.distance >= 10
      Equip das
    :?foe.armor >= 0 
      Equip bar
    : 
      Equipl vigor sw
      Equipr vigor sh
  :
    ?foe.distance > 10 
      Equip fire
    :?foe.count >= 3 | foe.distance <= 10
      Equip ba
//
?loc = mine // no idea
  
  ?foe = boss 
     Equip re
     
     ?foe.state = 32 &
     ^ foe.time = 20
       Equip mind 
     :?foe.state = 33 &
     ^ foe.time = 0
       equip dashing
     :?foe.state >= 33 &
     ^ foe.time >= 1 
       equip bar
       HEAL(heal)
   
   
  :?foe.distance <= 9 | foe.count >= 4
    Equip bar
  :
    equip re
  
  
//
?loc = halls // works on 7* ???
  ?foe = Booo | foe = Big booo
    Equipl vigor wa
    Equipr moon
  : 
    Equip ba
    ?foe = boss &
    ^ item.GetCoolDown(＂Bardiche +11 8*＂) <= 0 
      ?foe.hp <= 450 
        ?item.CanActivate(＂Bardiche +11 8*＂)
          Activate r
 
  ?foe ! immune_to_physical
    HEAL(heal)
//
?loc = Mushroom forest & loc.stars > 3
  ?foe = boss
    equip dash
    ?foe.armor > 0 
      Equip ba
    :
      Equipr co
      Equipl stone ham
  :
    ?foe.distance > 15 & foe.count <= 4
      Equip re
    :?foe.distance <= 9 
      Equip bar

?loc = temple 
// no idea what this works
  ?foe = boss 
    ?foe.state >= 1 | foe.state <= 111
      Equip bard
 
    ?foe.state = 112 
      Equip bard
      
      ?foe.time = 49 
        Equipl mind
        Equipr tri

      :?foe.time > 50 & foe.distance <= 5
        Equipl vigo
        Equipr ice
      :?foe.time > 50 & foe.distance >= 5 
        Equip re
      
      

      
      
    
    

  :
    ?foe = Cult Guard
