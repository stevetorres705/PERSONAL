var stat = 15
var stat2 = 5
// PRINT SCRIPTS 
>`@stat2@,@stat+1@,Use:@DisplayWeak(foe)@
>`@stat2@,@stat+2@,@foe.state@:@foe.time@ 
>`@stat2@,@stat+3@,
