// not really a func but 
// AAC FUNC
?item.left.state = 3 
  equipl 0* 
  equipl @item.left@
?item.right.state = 3
  equipr 0*
  equipr @item.right@
//
var BARD = ＂Bardiche +11 8*＂
?loc = deadwood & loc.stars > 3// works on 7*
  ?foe = boss 
    
    equip bar
    
    
    ?foe.state >= 32 & foe ! immune_to_stun
      Equipr co
      Equipl stone Hammer
     
    
    
  :?foe.distance >= 10 & foe ! immune_to_ranged
     Equip rep
    
  :?foe.count >= 3 | foe = immune_to_ranged
     Equip bard
    

 

    

 
//
?loc = caves // works on 7 *

  ?foe = boss
    
    Equipr ice
    Equipl vigor
  :?foe.distance < 10 | foe.count >= 4
    Equip bar
  :?foe.distance > 10 & foe.count <= 1
    Equip re
  HEAL(heal) 

//
?loc = icy // works on 5*
  ?foe = boss
    ?foe.distance >= 10
      Equip das
    :?foe.armor >= 0 
      Equip bar
    : 
      Equipl vigor sw
      Equipr vigor sh
  :
    ?foe.distance > 10 
      Equip fire
    :?foe.count >= 3 | foe.distance <= 10
      Equip ba
//
?loc = mine // no idea
  ?foe.distance <= 10 | foe.count >= 4
    Equip bar
  :?foe = boss 
     Equip re
     
     ?foe.state = 32 & foe.time = 20
       Equip mind 
     :?foe.time = 33 & foe.time = 10
       equip dashing
     :?foe.time = 33 & foe.time >= 11
       equip bar
   
   
  :
    equip re
  
  HEAL(heal)
//
?loc = halls // works on 7* ???
  ?foe = Booo | foe = Big booo
    Equipl vigor wa
    Equipr moon
  : 
    Equip ba
    ?foe = boss &
    ^ item.GetCoolDown(＂Bardiche +11 8*＂) <= 0 
      ?foe.hp <= 450 
        ?item.CanActivate(＂Bardiche +11 8*＂)
          Activate r
 
  ?foe ! immune_to_physical
    HEAL(heal)
//
?loc = Mushroom forest | loc = angry mushroom
  ?foe = boss
    equip dash
    ?foe.armor > 0 
      Equip ba
    :
      Equipr co
      Equipl stone ham
  :
    ?foe.distance > 15 & foe.count <= 4
      Equip re
    :?foe.distance <= 9 
      Equip bar
//
?loc = temple // no idea what this works on
  ?foe.distance < 10 & foe.count >= 3
    Equip bard
  :
    Equip re
//

?loc = rocky // 6*
 
  DYSANPROT()
 
  
